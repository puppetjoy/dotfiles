AddKeysToAgent yes
CanonicalizeHostname yes
CanonicalizeMaxDots 0
CanonicalDomains nest home eyrie

Host gateway.home
    User admin

Host unifi.home
    User root

Host github.com gitlab.joyfullee.me gitlab.localhost
    ForwardX11 no
    User git

Host *.nest *.eyrie
    ForwardAgent yes
    ForwardX11 yes
    ForwardX11Trusted yes
    ServerAliveInterval 60
